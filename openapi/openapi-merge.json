{
  "inputs": [
    {
      "inputFile": "./base.yaml"
    },
    {
      "inputFile": "./../ee/auth/openapi.yaml",
      "pathModification":{
        "prepend": "/api/auth"
      },
      "dispute": {
        "prefix": "auth"
      }
    },
    {
      "inputFile": "./../apps/ledger/openapi.yaml",
      "pathModification":{
        "prepend": "/api/ledger"
      },
      "dispute": {
        "prefix": "ledger"
      }
    },
    {
      "inputFile": "./../apps/payments/openapi.yaml",
      "pathModification":{
        "prepend": "/api/payments"
      },
      "dispute": {
        "prefix": "payments"
      }
    },
    {
      "inputFile": "./../ee/search/openapi.yaml",
      "pathModification":{
        "prepend": "/api/search"
      },
      "dispute": {
        "prefix": "search"
      }
    },
    {
      "inputFile": "./../ee/webhooks/openapi.yaml",
      "pathModification":{
        "prepend": "/api/webhooks"
      },
      "operationSelection": {
        "excludeTags": ["Health"]
      },
      "dispute": {
        "prefix": "webhooks"
      }
    },
    {
      "inputFile": "./../ee/wallets/openapi.yaml",
      "pathModification":{
        "prepend": "/api/wallets"
      },
      "operationSelection": {
        "excludeTags": ["Health"]
      },
      "dispute": {
        "prefix": "wallets"
      }
    },
    {
      "inputFile": "./../ee/orchestration/openapi.yaml",
      "pathModification":{
        "prepend": "/api/orchestration"
      },
      "operationSelection": {
        "excludeTags": ["Health"]
      },
      "dispute": {
        "prefix": "orchestration"
      }
    }
  ],
  "output": "./build/generate.json"
}
